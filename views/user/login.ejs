      <form action="/home" method="post">
        <!-- Email input -->
        <div class="form-outline mb-4">
        <label class="form-label" for="form2Example1">Username<sup style="color: red;">*</sup></label>
          <input
            type="text"
            id="username"
            class="form-control"
            name="username" placeholder="Enter your username"/>
        </div>

        <!-- Password input -->
        <div class="form-outline mb-4">
        <label class="form-label" for="form2Example2">Password<sup style="color: red;">*</sup></label>
          <div class="eye"> <input
            type="password"
            id="form2Example2"
            class="form-control"
            name="pass" placeholder="Enter your password" />
          <img src="../admin/assets/imgs/theme/eye-close.png" id="eyeIcon">
          </div>
          
        </div>

        <!-- 2 column grid layout for inline styling -->
        <div class="row mb-4">
          <div class="col">
            <!-- Simple link -->
            <a href="/forgotPassword">Forgot password?</a>
          </div>
        </div>

        <!-- Submit button -->
        <button type="submit" class="btn btn-primary btn-block mb-4">
          Sign in
        </button>

        <!-- Register buttons -->
        <div class="text-center">
          <p>Not a member? <a href="/register">Register</a></p>
        </div>
      </form>

